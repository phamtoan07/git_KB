	 <!--lay thong tin dang ky thu huong
		input: afacctno, backacc, v_type
		v_type = 0: output: thong tin nguoi thu huong cua MSI va lien ngan hang
		v_type = 1: output: chi tra ve thong tin thu huong cua lien ngan hang
		-->
	<TableInfo>
        <QueryKey>CHECKEXISTBENEFITBANK</QueryKey>
        <QueryValue>
            <![CDATA[
      select * from cfotheracc where afacctno=:PV_AFACCTNO and bankacc=:PV_BANKACC
		AND ((substr(bankid,1,3) <> '302' AND 1 =:v_type) OR 0 =:v_type)
      ]]>
        </QueryValue>
    </TableInfo>
	
